    "@types/lodash": "^4.14.198",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/node": "20.6.2",
    "@types/passport-github": "^1.1.8",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "prisma": "^5.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
