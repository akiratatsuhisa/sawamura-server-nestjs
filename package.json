    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/websockets": "^9.2.1",
    "@prisma/client": "^4.8.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
